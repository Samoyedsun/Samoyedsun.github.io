---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>samoyedsun.github.io - {{ page.name }}</title>
  <link href="http://samoyedsun.github.io"/>
  <link type="application/atom+xml" rel="self" href="http://samoyedsun.github.io/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://samoyedsun.github.io</id>
  <author>
    <name>hqwrong</name>
  </author>

  {% for post in site.posts %}
  {% if post.tag != "draft" %}
  {% if post.tag == page.tag or page.tag == "ALL" %}
      <entry>
        <id>http://samoyedsun.github.io{{ post.id }}</id>
        <link type="text/html" rel="alternate" href="http://samoyedsun.github.io{{ post.url }}"/>
        <title>{{ post.title | xml_escape }}</title>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <content type="html">{{ post.content | xml_escape }}</content>
      </entry>
  {% endif %}
  {% endif %}
  {% endfor %}
 
</feed>

